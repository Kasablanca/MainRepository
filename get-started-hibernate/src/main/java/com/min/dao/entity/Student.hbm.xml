<?xml version="1.0"?>  
<!DOCTYPE hibernate-mapping PUBLIC   
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.min.dao.entity.Student" table="t_student" >
		<id name="stuNo" column="stu_no" length="20" />
		<property name="stuName" column="stu_name" length="20" />
		<property name="sex" column="sex" />
		<property name="score" column="score" />
	</class>
</hibernate-mapping>