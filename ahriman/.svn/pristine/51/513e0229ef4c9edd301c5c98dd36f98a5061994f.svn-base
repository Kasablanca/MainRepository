<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.syhd.ahriman.dao.mapper.UserRegisteredMapper">
	<resultMap id="BaseResultMap"
		type="com.syhd.ahriman.dao.model.UserRegistered">
		<id column="date" jdbcType="DATE" property="date" />
		<id column="platform" jdbcType="VARCHAR" property="platform" />
		<result column="daily_registered" jdbcType="INTEGER"
			property="dailyRegistered" />
		<result column="total_registered" jdbcType="INTEGER"
			property="totalRegistered" />
	</resultMap>
	<sql id="Base_Column_List">
		date, platform, daily_registered, total_registered
	</sql>
	<select id="selectByPrimaryKey" parameterType="map"
		resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from t_user_registered
		where date = #{date,jdbcType=DATE}
		and platform = #{platform,jdbcType=VARCHAR}
	</select>
	<delete id="deleteByPrimaryKey" parameterType="map">
		delete from t_user_registered
		where date = #{date,jdbcType=DATE}
		and platform = #{platform,jdbcType=VARCHAR}
	</delete>
	<insert id="insert"
		parameterType="com.syhd.ahriman.dao.model.UserRegistered">
		insert into t_user_registered (date, platform, daily_registered,
		total_registered)
		values (#{date,jdbcType=DATE}, #{platform,jdbcType=VARCHAR},
		#{dailyRegistered,jdbcType=INTEGER},
		#{totalRegistered,jdbcType=INTEGER})
	</insert>
	<insert id="insertSelective"
		parameterType="com.syhd.ahriman.dao.model.UserRegistered">
		insert into t_user_registered
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="date != null">
				date,
			</if>
			<if test="platform != null">
				platform,
			</if>
			<if test="dailyRegistered != null">
				daily_registered,
			</if>
			<if test="totalRegistered != null">
				total_registered,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="date != null">
				#{date,jdbcType=DATE},
			</if>
			<if test="platform != null">
				#{platform,jdbcType=VARCHAR},
			</if>
			<if test="dailyRegistered != null">
				#{dailyRegistered,jdbcType=INTEGER},
			</if>
			<if test="totalRegistered != null">
				#{totalRegistered,jdbcType=INTEGER},
			</if>
		</trim>
	</insert>
	<update id="updateByPrimaryKeySelective"
		parameterType="com.syhd.ahriman.dao.model.UserRegistered">
		update t_user_registered
		<set>
			<if test="dailyRegistered != null">
				daily_registered = #{dailyRegistered,jdbcType=INTEGER},
			</if>
			<if test="totalRegistered != null">
				total_registered = #{totalRegistered,jdbcType=INTEGER},
			</if>
		</set>
		where date = #{date,jdbcType=DATE}
		and platform = #{platform,jdbcType=VARCHAR}
	</update>
	<update id="updateByPrimaryKey"
		parameterType="com.syhd.ahriman.dao.model.UserRegistered">
		update t_user_registered
		set daily_registered = #{dailyRegistered,jdbcType=INTEGER},
		total_registered = #{totalRegistered,jdbcType=INTEGER}
		where date = #{date,jdbcType=DATE}
		and platform = #{platform,jdbcType=VARCHAR}
	</update>
	<select id="getLastCountDate" resultType="java.util.Date">
		select max(date) from t_user_registered
	</select>
	<select id="getUserRegistered" resultType="com.syhd.ahriman.dao.model.UserRegistered">
		SELECT
			date,
			platform,
			(
			SELECT
				count( uid ) 
			FROM
				user_info 
			WHERE
				user_info.regTime &lt; DATE_ADD( t.date, INTERVAL 1 DAY ) 
				AND user_info.regType = t.platform 
			) totalRegistered,
			(
			SELECT
				count( uid ) 
			FROM
				user_info 
			WHERE
				date(user_info.regTime) = t.date
				AND user_info.regType = t.platform 
			) dailyRegistered
		FROM
			( SELECT DISTINCT date( regTime ) date, regType platform FROM user_info ) t
		WHERE date &lt; #{endDate}
		<if test="startDate != null">
			AND date &gt;= #{startDate}
		</if>
	</select>
	<insert id="batchInsert">
		insert into t_user_registered (date, platform, total_registered, daily_registered) values 
		<trim suffixOverrides=",">
			<foreach collection="list" item="item">
				(#{item.date,jdbcType=DATE},
				 #{item.platform,jdbcType=VARCHAR},
				 #{item.totalRegistered,jdbcType=INTEGER},
				 #{item.dailyRegistered,jdbcType=INTEGER}),
			</foreach>
		</trim>
	</insert>
</mapper>