Private Sub Worksheet_Change(ByVal Target As Range)
    Dim right As Range
    If Target.Column >= 11 And Target.Column <= 15 Then
        Set right = Target.Next
        Do While right.Column >= 11 And right.Column <= 15
            right.Value = ""
            Set right = right.Next
        Loop
    End If
End Sub
